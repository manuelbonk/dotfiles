URxvt.url-select.launcher:   chromium
URxvt.url-select.underline:  true
URxvt.keysym.C-i:            perl:url-select:select_next
URxvt.urgentOnBell:          true
! copy and paste w/ shift + {c,v}
URxvt.perl-ext-common:       default,clipboard,keyboard-select,selection-to-clipboard
URxvt.iso14755: False
URxvt.keysym.Shift-Control-V: perl:clipboard:paste

URxvt.borderless:            true
URxvt.scrollBar:             false

URxvt.font:					xft:PragmataPro:pixelsize=11:antialias=true
URxvt.boldFont:				xft:PragmataPro:style=bold:pixelsize=10:antialias=true


!URxvt.scrollstyle:           xterm
XTerm*.allowBoldFonts : true
XTerm*.bellIsUrgent   : true
XTerm*.cutNewline     : true
XTerm*.faceName       : PragmataPro
XTerm*.faceSize       : 8
XTerm*.fullscreen     : never
XTerm*.internalBorder : 0
XTerm*.renderFont     : true
XTerm*.termName       : xterm-256color
XTerm*.utf8           : always
XTerm*.utf8Fonts      : always
XTerm*.visualBell     : false
XTerm*.visualBellLine : false
