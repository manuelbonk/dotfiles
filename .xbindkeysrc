# toggle mute
#"amixer -D pulse set Master 1+ toggle"
"pulseaudio-ctl mute"
	XF86AudioMute

# decrease volume
#"amixer set Master playback 5-"
"pulseaudio-ctl down"
	XF86AudioLowerVolume


# increase volume
#"amixer set Master playback 5+"
"pulseaudio-ctl up"
	XF86AudioRaiseVolume

# decrease brightness
"brightnessctl s 5%-"
	F8

# increase brightness
"brightnessctl s 5%+"
	F9

"xmodmap ~/.Xmodmap"
	F12
