URxvt.keysym.Shift-Control-V: perl:clipboard:paste
URxvt.iso14755: False
URxvt.perl-ext-common: default,clipboard
URxvt.scrollBar: false

URxvt*font: xft:PragmataPro:pixelsize=13
!URxvt*boldFont: xft:Input Bold:pixelsize=11
!URxvt.letterSpace: -4
!URxvt*font: xft:Monaco:pixelsize=11
!URxvt*boldFont: xft:Monaco Bold:pixelsize=11
!
! invisibone by baskerville
!
! vim: set ft=xdefaults:

!(235, 248)
URxvt.background: #262626
URxvt.foreground: #A8A8A8

!(252)
URxvt.colorBD:    #D0D0D0
URxvt.colorIT:    #D0D0D0
URxvt.colorUL:    #D0D0D0

!BLK (236, 243)
URxvt.color0:     #303030
URxvt.color8:     #767676

!WHT (252, 231)
URxvt.color7:     #D0D0D0
URxvt.color15:    #FFFFFF

!RED
URxvt.color1:     #EA1010
URxvt.color9:     #FFA7DA

!GRN
URxvt.color2:     #6D9E3F
URxvt.color10:    #A3D572

!BLU
URxvt.color4:     #6095C5
URxvt.color12:    #98CBFE

!YEL
URxvt.color3:     #B58858
URxvt.color11:    #EFBD8B

!CYN
URxvt.color6:     #3BA275
URxvt.color14:    #75DAA9

!MAG
URxvt.color5:     #AC7BDE
URxvt.color13:    #E5B0FF

! hide pointer while typing
URxvt.pointerBlank: true
! URxvt.letterSpace: -2
!
urxvt.depth: 8

xterm*background: rgb:26/26/26
xterm*foreground: rgb:A8/A8/A8
xterm*faceName: Monaco:size=9:antialias=true
!URxvt.transparent:true
!URxvt:shading:20
